<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="footer-content">
            <block class="Magento\Newsletter\Block\Subscribe" name="form.subscribe" as="subscribe" after="-"
                   template="Magento_Newsletter::subscribe.phtml" ifconfig="newsletter/general/active">
                <arguments>
                    <argument name="form_key_view_model" xsi:type="object">Wigman\Tailwind\ViewModel\FormKey</argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
